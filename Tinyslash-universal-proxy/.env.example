# Pebly Universal Proxy Environment Configuration
# Copy this file to .env and update the values

# Backend URL - Your URL shortener service
BACKEND_URL=https://urlshortner-1-hpyu.onrender.com

# Proxy Configuration
PROXY_VERSION=2.0
CACHE_TTL=300

# Development Settings
DEV_BACKEND_URL=http://localhost:8080

# Analytics (Optional)
ANALYTICS_ENDPOINT=https://your-analytics-service.com/api/events

# Monitoring (Optional)
HEALTH_CHECK_INTERVAL=60000
ERROR_THRESHOLD=5

# Custom Domain (Optional)
CUSTOM_DOMAIN=proxy.yourdomain.com
ZONE_NAME=yourdomain.com